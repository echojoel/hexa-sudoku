    <!-- <br> -->
    <div class="container">
        <table id="grid">
          <tr>
          <% const Nrows = Math.sqrt(solvedSudoku.length)%>
          <% var bgColor %>
          <% for (var i = 0; i < Nrows; i++) { %>
            <tr> 
            <% for (var j = 0; j < Nrows; j++) { %>
              <% if (initialSudoku[j + i*Nrows] != solvedSudoku[j + i*Nrows]) {bgColor="#90ee90"} else {bgColor="#ffffff"}%>
              <td bgcolor="<%=bgColor%>"><%= solvedSudoku[j + i*Nrows] %></td>
            <% } %>
            </tr>
          <% } %>
        </tr>
         
        </table>
        </div>
      </br>
        <p><i>Solved in: <%= parseFloat(time).toFixed(2) %> Seconds</i></p>
      <!-- </br> -->

    </center>
</body>


